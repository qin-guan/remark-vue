{
  "name": "@qingu/remark-vue",
  "type": "module",
  "version": "1.0.8",
  "description": "Convert markdown to Vue",
  "author": "Qin Guan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/qin-guan/remark-vue"
  },
  "keywords": [
    "remark",
    "markdown",
    "unified",
    "vue",
    "rehype",
    "hast"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --cache .",
    "lint:fix": "eslint --cache --fix ."
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "mdast-util-to-hast": "^12.3.0",
    "rehype-vue": "^1.0.2",
    "remark-gfm": "^3.0.1",
    "remark-parse": "^10.0.2",
    "remark-rehype": "^10.1.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.7",
    "@types/hast": "^2.3.4",
    "eslint": "^8.44.0",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "unified": "^10.1.2"
  }
}
